@using CarDealership.Common

@if (TempData[Constants.MessageConstants.ErrorMessage] != null)
{
    <script>
        $(function () {
            toastr.error(`@TempData[Constants.MessageConstants.ErrorMessage].ToString()`);
        });
    </script>
}

@if (TempData[Constants.MessageConstants.WarningMessage] != null)
{
    <script>
        $(function () {
            toastr.warning(`@TempData[Constants.MessageConstants.WarningMessage].ToString()`);
        });
    </script>
}

@if (TempData[Constants.MessageConstants.SuccessMessage] != null)
{
    <script>
        $(function () {
            toastr.success(`@TempData[Constants.MessageConstants.SuccessMessage].ToString()`);
        });
    </script>
}
